<%- include layouts/header %>
        <!-- Main content -->
        <section class="content" ng-controller="mainController">
          <div class="row">
            <div class="col-xs-12">
              <div class="col-xs-6 col-xs-offset-3">
                <div class="box" style="padding-bottom: 5px;">
                  <div class="box-body" >
                    <div class="box-header">
                      <center><h3 class="box-title">Mandals List</h3></center>
                    </div><!-- /.box-header -->
                    <div class="box-body ng-cloak" ng-init="getMandals()">
                      <table id="example2" class="table">
                        <tbody>
                          <tr ng-repeat="mandal in mandals track by $index">
                            <td>{{$index + 1}}</td>
                            <td><a href="/dashboard/list/{{mandal.url}}">{{mandal.name}}</a></td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div><!-- /.box-body -->
                </div><!-- /.box -->
              </div>

            </div><!-- /.col -->
          </div><!-- /.row -->
        </section><!-- /.content -->
﻿<%- include layouts/footer %>
