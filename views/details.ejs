<%- include layouts/header %>
    <section class="content-header">
      <h1>
        Dashboard
      </h1>
      <ol class="breadcrumb">
        <li><a href="/dashboard"><i class="fa fa-dashboard"></i> Dashboard</a></li>
        <li class="active">Details</li>
      </ol>
    </section>
        <!-- Main content -->
        <section class="content" ng-controller="mainController">
          <% if (id) { %>
          <script>
            var aadhaarNo;
            var id = JSON.parse('<%- JSON.stringify(id) %>');
          </script>
          <% } else if(aadhaarNo) { %>
            <script>
              var id;
              var aadhaarNo = JSON.parse('<%- JSON.stringify(aadhaarNo) %>');
            </script>
          <% } %>
          <div class="row">
            <div class="col-xs-12">
              <div class="row">
            <div class="ng-cloak col-md-6 col-md-offset-3">

              <div class="box box-info" ng-init="getData()">
                <div class="box-header" ng-show="data">
                  <h3 class="box-title">{{data.name}} details</h3>
                    <div class="btn-group pull-right">
                      <button class="btn btn-default btn-sm btn-info" ng-click="edit = true"><i class="fa fa-edit"></i></button>
                      <button class="btn btn-default btn-sm btn-success" ng-click="print(data)"><i class="fa fa-print"></i></button>
                    </div>
                </div>
                <div class="box-body" ng-show="data">
                  <!-- Color Picker -->
                  <div class="form-group">
                    <label>Reference ID:</label>
                    <span ng-if="!edit">{{data.referenceID}}</span>
                  </div><!-- /.form group -->

                  <!-- Color Picker -->
                  <div class="form-group">
                    <label>Name:</label>
                    <span ng-if="!edit">{{data.name}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-user"></i>
                      </div>
                      <input type="text" name="name" ng-model="data.name" class="form-control pull-right" placeholder="Name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>F/H Name:</label>
                    <span ng-if="!edit">{{data.father}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="father" ng-model="data.father" class="form-control pull-right" placeholder="Father/Husband Name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->
                  <div class="form-group">
                    <label>Aadhaar No:</label>
                    <span ng-if="!edit">{{data.aadhaar}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="number" name="aadhaar" ng-model="data.aadhaar" class="form-control pull-right" placeholder="Aadhaar No">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Mobile No:</label>
                    <span ng-if="!edit">{{data.mobile}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="number" name="mobile" ng-model="data.mobile" class="form-control pull-right" placeholder="Mobile">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Address:</label>
                    <span ng-if="!edit">{{data.address.village}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <textarea name="address.village" ng-model="data.address.village" rows="2" class="form-control pull-right" placeholder="Address"></textarea>
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Mandal:</label>
                    <span ng-if="!edit">{{data.address.mandal}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <select class="form-control" ng-model="data.address.mandal" name="address.mandal">
                        <option ng-repeat="mandal in mandals" value="{{mandal.name}}" >{{mandal.name}}</option>
                      </select>
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Proceeding No:</label>
                    <span ng-if="!edit">{{data.proceeding.no}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="proceeding.no" ng-model="data.proceeding.no" class="form-control pull-right" placeholder="Proceeding No">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>S.No of Proceeding:</label>
                    <span ng-if="!edit">{{data.proceeding.sNo}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="proceeding.sNo" ng-model="data.proceeding.sNo" class="form-control pull-right" placeholder="S.No of Proceeding">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Grounding Date:</label>
                    <span ng-if="!edit">{{data.grounding.date}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="grounding.date" ng-model="data.grounding.date" class="form-control pull-right" placeholder="DD/MM/YYYY">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Grounding Place:</label>
                    <span ng-if="!edit">{{data.grounding.place}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="grounding.place" ng-model="data.grounding.place" class="form-control pull-right" placeholder="Place of Grounding">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Seller Name:</label>
                    <span ng-if="!edit">{{data.seller.name}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="seller.name" ng-model="data.seller.name" class="form-control pull-right" placeholder="Seller Name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Seller Father name:</label>
                    <span ng-if="!edit">{{data.seller.father}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="seller.father" ng-model="data.seller.father" class="form-control pull-right" placeholder="Seller Father name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Seller Village:</label>
                    <span ng-if="!edit">{{data.seller.village}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="seller.village" ng-model="data.seller.village" class="form-control pull-right" placeholder="Seller Village name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Seller Aadhaar No:</label>
                    <span ng-if="!edit">{{data.seller.aadhaar}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="number" name="seller.aadhaar" ng-model="data.seller.aadhaar" class="form-control pull-right" placeholder="Seller Aadhaar no">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Gender:</label>
                    <span ng-if="!edit">{{data.gender}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <select class="form-control pull-right" ng-model="data.gender" name="gender">
                        <option value="male">Male</option>
                        <option value="female">Female</option>
                      </select>
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Caste:</label>
                    <span ng-if="!edit">{{data.caste}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="caste" ng-model="data.caste" class="form-control pull-right" placeholder="Caste">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Income:</label>
                    <span ng-if="!edit">{{data.income}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="number" name="income" ng-model="data.income" class="form-control pull-right" placeholder="Income">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Disability:</label>
                    <span ng-if="!edit">{{data.disability}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <select class="form-control pull-right" ng-model="data.disability" name="disability">
                        <option value="false">No</option>
                        <option value="true">Yes</option>
                      </select>
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Bank Name:</label>
                    <span ng-if="!edit">{{data.bank.name}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="bank.name" ng-model="data.bank.name" class="form-control pull-right" placeholder="Bank Name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>IFSC Code:</label>
                    <span ng-if="!edit">{{data.bank.ifsc}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="bank.ifsc" ng-model="data.bank.ifsc" class="form-control pull-right" placeholder="Bank IFSC Code">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Account No:</label>
                    <span ng-if="!edit">{{data.bank.account}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="bank.account" ng-model="data.bank.account" class="form-control pull-right" placeholder="Bank Account">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Amount Paid:</label>
                    <span ng-if="!edit">{{data.amountPaid}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="number" name="amountPaid" ng-model="data.amountPaid" class="form-control pull-right" placeholder="Amount Paid">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Cheque No:</label>
                    <span ng-if="!edit">{{data.chequeNo}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="chequeNo" ng-model="data.chequeNo" class="form-control pull-right" placeholder="Cheque No">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Transporter Name:</label>
                    <span ng-if="!edit">{{data.transporterName}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="transporterName" ng-model="data.transporterName" class="form-control pull-right" placeholder="Transporter Name">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Vehicle No:</label>
                    <span ng-if="!edit">{{data.vehicleNo}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="vehicleNo" ng-model="data.vehicleNo" class="form-control pull-right" placeholder="Vehicle No">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Date of Transport:</label>
                    <span ng-if="!edit">{{data.dateOfTransport}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="dateOfTransport" ng-model="data.dateOfTransport" class="form-control pull-right" placeholder="DD/MM/YYYY">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <div class="form-group">
                    <label>Applied Date:</label>
                    <span ng-if="!edit">{{data.appliedDate}}</span>
                    <div class="input-group" ng-if="edit">
                      <div class="input-group-addon">
                        <i class="fa fa-clock-o"></i>
                      </div>
                      <input type="text" name="appliedDate" ng-model="data.appliedDate" class="form-control pull-right" placeholder="DD/MM/YYYY">
                    </div><!-- /.input group -->
                  </div><!-- /.form group -->

                  <!-- time Picker -->
                  <div class="form-group">
                    <center><button type="button" class="btn btn-success" ng-click="update()">Update</button></center>
                  </div>
                </div><!-- /.box-body -->
                <div class="box-body" ng-show="!data">
                  <center><h4>No data found</h4></center>
                </div>
              </div><!-- /.box -->
          </div><!-- /.row -->
        </section><!-- /.content -->

﻿<%- include layouts/footer %>
